{
  "name": "@workforce/convex",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./*": {
      "types": "./src/*.ts",
      "default": "./src/*.ts"
    },
    "./api": {
      "types": "./convex/_generated/api.d.ts",
      "default": "./convex/_generated/api.js"
    }
  },
  "scripts": {
    "dev": "convex dev",
    "deploy": "convex deploy",
    "build": "tsc --noEmit"
  },
  "dependencies": {
    "convex": "^1.31.2"
  },
  "devDependencies": {
    "typescript": "^5.7.2"
  }
}

